proc keywords    {args}             {return ""}
proc example     {code}             {return ""}
proc example_begin {}               {return ""}
proc example_end {}                 {return ""}
proc nl          {}                 {return ""}
proc arg         {text}             {return ""}
proc cmd         {text}             {return ""}
proc opt         {text}             {return ""}
proc emph        {text}             {return ""}
proc strong      {text}             {return ""}
proc comment     {text}             {return ""}
proc sectref     {text}             {return ""}
proc syscmd      {text}             {return ""}
proc method      {text}             {return ""}
proc option      {text}             {return ""}
proc widget      {text}             {return ""}
proc fun         {text}             {return ""}
proc type        {text}             {return ""}
proc package     {text}             {return ""}
proc class       {text}             {return ""}
proc var         {text}             {return ""}
proc file        {text}             {return ""}
