# Defines the procedures a manpage rules file has to support for good
# manpages. The procedures here return nothing.

################################################################

proc manpage_begin {title section version} {return ""}
proc moddesc     {desc}             {return ""}
proc titledesc   {desc}             {return ""}
proc manpage_end {}                 {return ""}
proc require     {pkg {version {}}} {return ""}
proc description {}                 {return ""}
proc section     {name}             {return ""}
proc para        {}                 {return ""}
proc list_begin  {what}             {return ""}
